{
  "name": "forgerock-web-login-framework",
  "version": "0.0.1",
  "scripts": {
    "check": "npm run check:lint && npm run check:svelte",
    "check:lint": "prettier --ignore-path .gitignore -w --plugin-search-dir=. . && eslint --fix --ignore-path .gitignore .",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.json --threshold=warning",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "chromatic": "chromatic --project-token=e10acf0c74f9 --force-rebuild",
    "ci": "vitest --dir src",
    "ci:e2e": "playwright test",
    "ci:e2e:debug": "PWDEBUG=1 playwright test",
    "clean": "git clean -fdX -e \"!.env\"",
    "dev": "npm run build:widget && npm run dev:widget | npm run dev:app",
    "dev:app": "vite dev",
    "dev:style": "postcss --watch src/lib/widget/main.css --output package/widget.css",
    "dev:widget": "rollup -cw",
    "build": "npm run build:widget && npm run build:app",
    "build:app": "vite build",
    "build:storybook": "build-storybook",
    "build:widget": "rollup -c",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "storybook": "start-storybook --no-open -p 6006",
    "test": "vitest --dir src",
    "test:e2e": "npm run build && playwright test",
    "test:storybook": "test-storybook"
  },
  "dependencies": {
    "@forgerock/javascript-sdk": "^3.2.2",
    "uuid": "^8.3.2",
    "xss": "^1.0.13",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@playwright/test": "^1.27.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/jest": "^0.0.10",
    "@storybook/svelte": "^6.5.10",
    "@storybook/test-runner": "^0.5.0",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.10",
    "@sveltejs/adapter-auto": "^1.0.0-next.71",
    "@sveltejs/kit": "1.0.0-next.470",
    "@sveltejs/vite-plugin-svelte": "^1.0.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-basic-ssl": "^0.1.1",
    "@vitest/ui": "^0.12.6",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.5.4",
    "color": "^4.2.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^27.5.1",
    "markdown-toc": "^1.2.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.0",
    "rimraf": "3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "7.0.2",
    "storybook-addon-code-editor": "^0.3.5",
    "storybook-dark-mode": "1.1.0",
    "svelte": "^3.50.0",
    "svelte-check": "^2.9.0",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.16",
    "tailwindcss": "^3.1.8",
    "tslib": "^2.3.1",
    "typescript": "~4.7.4",
    "vite": "^3.1.0",
    "vitest": "^0.16.0"
  },
  "type": "module",
  "engines": {
    "node": ">= 16.0.0 <17.0.0",
    "npm": ">=8.0.0"
  },
  "volta": {
    "node": "16.17.0"
  }
}
